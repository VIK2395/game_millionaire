{
    "env": {
        "browser": true,
        "es2020": true
    },
    "parser": "babel-eslint",
    "extends": [
        //"airbnb",
        //"plugin:react/recommended",
        "prettier"

        /*"prettier/react"*/
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        //"react",
        "prettier"
    ],
    "rules": {
        "prettier/prettier": "off",
        "no-unused-vars": "off",
        "no-console": "off",
        "no-param-reassign": "off",
        "no-plusplus": "off"
    }
}
